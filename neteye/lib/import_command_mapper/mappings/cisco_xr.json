{
    "import_serial":[
            {
                "command": "admin show inventory",
                "field" : {
                    "serial_number": "sn",
                    "product_id": "pid",
                    "description": "descr"
                },
                "index": null
            }
    ],
    "import_node": [
        {
            "command": "show version",
            "field": {
                "model": "hardware",
                "os_version": "version"
            },
            "index": 0
        }
    ],
    "import_interface": [
        {
            "command": "show interfaces",
            "field": {
                "name": "interface",
                "status": "link_status",
                "ip_address": "ip_address",
                "duplex": "duplex",
                "speed": "speed",
                "mtu": "mtu",
                "description": "description"
            },
            "index": null
        }
    ],
    "import_arp_entry": [
        {
            "command": "show arp",
            "field": {
                "ip_address": "ip_address",
                "mac_address": "mac_address",
                "arp_type": "type"
            },
            "ignore": [
                {
                    "age": "-"
                }
            ],
            "index": null
        }
    ]
}
